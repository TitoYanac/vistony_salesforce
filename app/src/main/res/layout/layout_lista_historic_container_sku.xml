<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="?attr/listPreferredItemHeight"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="8dp"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="8dp"
    android:id="@+id/relativeListahistoriccontainersalessku"
    >

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity=""
        android:focusable="false">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:focusable="false">



            <TextView
                android:id="@+id/lbl_montoacumulado"
                style="@android:style/TextAppearance.Widget.TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:text="@string/total"
                android:textAllCaps="true"
                android:textAlignment="center"

                android:textSize="14dp">

            </TextView>
            <TextView
                android:id="@+id/tv_montoacumulado"
                style="@android:style/TextAppearance.Widget.TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:text="@string/total"
                android:textAlignment="center"
                android:layout_marginLeft="10dp"
                android:textSize="14dp"
                android:textStyle="bold">

            </TextView>
            <TextView
                android:id="@+id/lbl_periodo"
                style="@android:style/TextAppearance.Widget.TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:text="@string/lbl_lastpurchase"
                android:textAllCaps="true"
                android:textAlignment="center"
                android:gravity="center"
                android:layout_gravity="center"
                android:textSize="14dp"
                android:layout_marginLeft="20dp">
            </TextView>
            <TextView
                android:id="@+id/tv_periodo"
                style="@android:style/TextAppearance.Widget.TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:text="@string/lbl_lastpurchase"
                android:textSize="14dp"
                android:textStyle="bold">
            </TextView>

        </TableRow>
    </TableLayout>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity=""
        android:focusable="false">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:focusable="false">
            <TextView
                android:id="@+id/lbl_galouns"
                style="@android:style/TextAppearance.Widget.TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:text="@string/gallon"
                android:textAllCaps="true"
                android:textAlignment="center"
                android:textSize="14dp"
                android:layout_marginTop="20dp">

            </TextView>

            <TextView
                android:id="@+id/tv_galouns"
                style="@android:style/TextAppearance.Widget.TextView"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:text="@string/gallon"
                android:textSize="14dp"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:textStyle="bold">
            </TextView>
        </TableRow>
    </TableLayout>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity=""
        android:focusable="false"
        android:layout_marginTop="40dp"
        >
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="false">
        <TextView
            android:id="@+id/lbl_variable"
            style="@android:style/TextAppearance.Widget.TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:text="@string/article"
            android:textAllCaps="true"
            android:textAlignment="center"
            android:textSize="14dp">

        </TextView>

        <TextView
            android:id="@+id/tv_variable"
            style="@android:style/TextAppearance.Widget.TextView"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:text="@string/article"
            android:textSize="14dp"
            android:textStyle="bold">
        </TextView>
    </TableRow>
    </TableLayout>

</RelativeLayout>