<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeListaPriceList"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="?attr/listPreferredItemHeight"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="8dp"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="8dp">
    <TextView

        android:id="@+id/lbl_historico_facturas_facturacion_documento_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/lbl_code"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/tv_documento_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginLeft="120dp"
        android:textStyle="bold"
        android:text="Codigo1"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/lbl_historico_facturas_facturacion_nrofactura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:text="Documento:"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/tv_legalnumber"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="20dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="Documento1"
        android:textSize="18dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/lbl_historico_facturas_facturacion_fechaemision"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="40dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/lbl_f_emision"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/tv_dateemition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="120dp"
        android:textStyle="bold"
        android:text="1900-01-01"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/lbl_historico_facturas_facturacion_montoimporte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="20dp"
        android:text="Importe:"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/tv_amount_invoice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="120dp"
        android:textStyle="bold"
        android:text="0"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/lbl_historico_facturas_facturacion_saldo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:text="Saldo:"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/tv_balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="120dp"
        android:textStyle="bold"
        android:text="0"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/lbl_historico_facturas_facturacion_lineas_no_facturadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="100dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="Tipo:"
        android:visibility="gone"
        android:textSize="18dp" />
    <TextView
        android:id="@+id/tv_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="120dp"
        android:textStyle="bold"
        android:visibility="gone"
        android:text="0"
        android:textSize="18dp" />
    <ImageView
        android:id="@+id/imv_historico_orden_venta_facturacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="220dp"
        android:layout_marginTop="100dp"
        android:visibility="invisible"
        app:srcCompat="@drawable/ic_baseline_article_24" />

</RelativeLayout>
