<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:minHeight="?attr/listPreferredItemHeight"
android:orientation="vertical"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingTop="8dp"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingBottom="8dp"
android:id="@+id/realtive_ruta">


    <androidx.cardview.widget.CardView
        android:id="@+id/cv_comision_historic_sales_analysis_by_route"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >
        <TextView
            android:id="@+id/tv_class_commercial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:text="Variable1"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textSize="25dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lbl_month_current"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="40dp"
            android:text="Periodo Actual - GAL"
            android:textSize="20dp"

            android:textAppearance="?attr/textAppearanceListItem"
            />

        <TextView
            android:id="@+id/tv_month_current"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="240dp"
            android:layout_marginTop="40dp"
            android:text="Variable1"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textAlignment="textEnd"
            android:textAppearance="?attr/textAppearanceListItem"

            />
        <View
            android:id="@+id/v_gradiente_historic_sales_analysis_by_route"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/linea_rectangular_gradiente_muy_bajo" />

        <TextView
            android:id="@+id/lbl_indicator_month_before"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="90dp"
            android:text="@string/lbl_indicator_month_before"
            android:textAppearance="?attr/textAppearanceListItem" />

        <TextView
            android:id="@+id/tv_indicator_month_before"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="90dp"
            android:text="@string/lbl_indicator_month_before"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textStyle="bold" />



        <TextView
            android:id="@+id/lbl_indicator_biannual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="120dp"
            android:text="@string/lbl_indicator_biannual"
            android:textAppearance="?attr/textAppearanceListItem" />

        <TextView
            android:id="@+id/tv_indicator_biannual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="120dp"
            android:text="@string/lbl_indicator_biannual"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lbl_quota_percent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="150dp"
            android:text="@string/lbl_indicator_percent_quota"
            android:textAppearance="?attr/textAppearanceListItem" />

        <TextView
            android:id="@+id/tv_quota_percent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="150dp"
            android:text="@string/lbl_indicator_percent_quota"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="190dp"
            android:background="@drawable/borde_cuadrado"
            android:orientation="vertical"></LinearLayout>

        <ImageView
            android:id="@+id/imv_show_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="300dp"
            android:layout_marginTop="170dp"
            android:textColor="@color/white"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_expand_more_24" />

        <ImageView
            android:id="@+id/imv_detail_mes_anterior"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="90dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_visibility_24" />
        <ImageView
            android:id="@+id/imv_detail_semestral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="120dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_visibility_24" />

        <ImageView
            android:id="@+id/imv_detail_cuota"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="150dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_visibility_24" />
        <ImageView
            android:id="@+id/imv_alert_detail_mes_anterior"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="90dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_dangerous_24" />
        <ImageView
            android:id="@+id/imv_alert_detail_semestral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="120dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_dangerous_24" />
        <ImageView
            android:id="@+id/imv_alert_detail_cuota"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="150dp"
            android:textColor="@color/Black"
            android:textStyle="bold"
            app:srcCompat="@drawable/ic_baseline_dangerous_24" />

        <TextView
            android:id="@+id/lbl_percent_quote"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="150dp"
            android:text="@string/lbl_percent"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/lbl_percent_semestral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="120dp"
            android:text="@string/lbl_percent"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/lbl_percent_anio_anterior"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="220dp"
            android:layout_marginTop="90dp"
            android:text="@string/lbl_percent"
            android:textAppearance="?attr/textAppearanceListItem"
            android:textStyle="bold" />
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>